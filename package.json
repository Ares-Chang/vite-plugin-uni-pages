{
  "name": "@uni-helper/vite-plugin-uni-pages",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@7.14.0",
  "description": "File system based routing for uni-app applications using Vite",
  "author": "KeJun",
  "license": "MIT",
  "homepage": "https://github.com/uni-helper/vite-plugin-uni-pages#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/vite-plugin-uni-pages.git"
  },
  "bugs": "https://github.com/uni-helper/vite-plugin-uni-pages/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "require": "./dist/config/index.cjs",
      "import": "./dist/config/index.js"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "play": "nr -C playground dev:h5",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish --access public",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "bumpp": "^8.2.1",
    "tsup": "^6.5.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "@vue/compiler-dom": "^3.2.45",
    "colorette": "^2.0.19",
    "consola": "^2.15.3",
    "defu": "^6.1.0",
    "fast-glob": "^3.2.12",
    "jsonc-parser": "^3.2.0",
    "unconfig": "^0.3.7"
  }
}
